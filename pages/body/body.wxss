/*滚动条   */

tab-scrollx {
  height: 110rpx;
  line-height: 80rpx;
  background: white;
  font-family: PingFang;
  margin: 0 auto;
  color: #279cff;
  border-bottom: 2rpx #f7f7f7 solid;
  position: fixed;
  z-index: 1;
  top: 0;
}

radio .wx-radio-input {
  width: 14px;
  height: 14px;
}

radio .wx-radio-input.wx-radio-input-checked::before {
  transform: translate(-50%, -48%) scale(0.53);
  -webkit-transform: translate(-50%, -48%) scale(0.53);
}

.tab-item {
  width: 33.3%;
  display: inline-block;
  text-align: center;
  color: #b3b3b3;
  font-size: 34rpx;
  font-family: PingFang;
  margin-top: 12rpx;
}

.tab-item.active {
  color: #279cff;
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  background: #279cff;
  position: absolute;
  bottom: 0;
  left: 84rpx;
  align-items: center;
  border-radius: 2rpx;
}

.tab-content {
  height: 470rpx;
  width: 100%;
  background-color: #fff;
  margin-top: 20rpx;
}

.scroll-y {
  height: 100%;
}

